            <% if @user.itineraries.any? %>
                    <div class="cards-container-profile">
                        <% @published_itineraries.each do |itinerary| %>
                            <div class="itinerary-card">
                                <div id="carousel_<%= itinerary.id %>" class="carousel slide" data-interval="false">
                                    <ol class="carousel-indicators">
                                        <li data-target="#carousel_<%= itinerary.id %>" data-slide-to="0" class="active"></li>
                                        <li data-target="#carousel_<%= itinerary.id %>" data-slide-to="1"></li>
                                        <li data-target="#carousel_<%= itinerary.id %>" data-slide-to="2"></li>
                                    </ol>
                                    <div class="carousel-inner">
                                        <% itinerary.photos.each_with_index do |photo, index| %>
                                        <div class="carousel-item <%= 'active' if index.zero? %>">
                                            <%= cl_image_tag(photo.key, class: "d-block w-100" ) %>
                                        </div>
                                        <% end %>
                                    </div>
                                    <a class="carousel-control-prev" href="#carousel_<%= itinerary.id %>" role="button" data-slide="prev">
                                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" href="#carousel_<%= itinerary.id %>" role="button" data-slide="next">
                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Next</span>
                                    </a>
                                </div>
                                <div class="name-duration">
                                    <div class="i-name">
                                        <%= link_to itinerary.name, itinerary_path(itinerary) %>
                                    </div>
    <!----------------- ---------Button trigger modal -->
                                    <% if itinerary.publish == true %>
                                        <div>
                                            <button type="button" class="button-modal" data-toggle="modal" data-target="#exampleModalCenter">
                                            <i class="fas fa-check-square tick-green"></i>
                                            </button>
                                        </div>                                
                                    <% else %>
                                        <div>
                                            <button type="button" class="button-modal" data-toggle="modal" data-target="#exampleModal">
                                            <i class="fas fa-minus-square icon-orange"></i>
                                            </button>
                                        </div>    
                                    <% end %>    
    <!------------------------------ Modal ---------------------------------------->
                                    <% if itinerary.publish == true %>
                                        <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                                        <div class="modal-dialog modal-dialog-centered" role="document">
                                            <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="exampleModalLongTitle"><i class="fas fa-check-square tick-green-modal"></i></h5>
                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                            </div>
                                            <div class="modal-body">
                                                This itinerary is published. Other Travelizers can read it and contact you with any questions. If your itinerary is not finished yet, you can unpublish it any time.
                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="search-button-modal" data-dismiss="modal">Close</button>
                                                <button type="button" class="search-button-modal"><%= link_to "Unpublish", edit_itinerary_path(itinerary)%></button>
                                            </div>
                                            </div>
                                        </div>
                                        </div>
                                    <% else %>
    <!------------------------------ Modal ------------------------------------------>
                                        <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                                        <div class="modal-dialog modal-dialog-centered" role="document">
                                            <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="exampleModalLongTitle"><i class="fas fa-minus-square icon-orange"></i></h5>
                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                                </button>
                                            </div>
                                            <div class="modal-body">
                                                This itinerary has not been published yet. If you are happy with it, you can publish it now!
                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="search-button-modal" data-dismiss="modal">Close</button>
                                                <button type="button" class="search-button-modal"><%= link_to "Publish", edit_itinerary_path(itinerary)%></button>
                                            </div>
                                            </div>
                                        </div>
                                        </div>
                                    <% end %>
    <!------------------------------ End of Modal --------------------------------------->
                                </div>  
                            </div>             
                        <%end%>
                        <%end%>